{
  "clientId": "odh-data-quality-web",
  "name": "Open Data Hub Automated Data Quality Monitoring Tool",
  "description": "Client for login into ADQMT web application",
  "rootUrl": "",
  "adminUrl": "",
  "baseUrl": "https://data-quality.opendatahub.testingmachine.eu",
  "surrogateAuthRequired": false,
  "enabled": true,
  "alwaysDisplayInConsole": false,
  "clientAuthenticatorType": "client-secret",
  "redirectUris": [
    "https://data-quality.opendatahub.testingmachine.eu",
    "http://localhost:8080"
  ],
  "webOrigins": [
    "",
    "https://data-quality.opendatahub.testingmachine.eu",
    "http://localhost:8080"
  ],
  "notBefore": 0,
  "bearerOnly": false,
  "consentRequired": false,
  "standardFlowEnabled": true,
  "implicitFlowEnabled": false,
  "directAccessGrantsEnabled": false,
  "serviceAccountsEnabled": false,
  "publicClient": true,
  "frontchannelLogout": false,
  "protocol": "openid-connect",
  "attributes": {
    "access.token.lifespan": "7200",
    "saml.force.post.binding": "false",
    "saml.multivalued.roles": "false",
    "post.logout.redirect.uris": "+",
    "oauth2.device.authorization.grant.enabled": "false",
    "backchannel.logout.revoke.offline.tokens": "false",
    "saml.server.signature.keyinfo.ext": "false",
    "use.refresh.tokens": "true",
